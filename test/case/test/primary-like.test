SELECT * FROM like_table WHERE name LIKE 'c%';
SELECT * FROM like_table WHERE name NOT LIKE 'c%';
SELECT * FROM like_table WHERE name LIKE 'a%';
SELECT * FROM like_table WHERE name LIKE 'c%a';
SELECT * FROM like_table WHERE name NOT LIKE 'c%a';
